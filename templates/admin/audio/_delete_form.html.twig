<form method="post" action="{{ path('admin_audio_delete', {'id': audio.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette musique ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ audio.id) }}">
    <button class="btn-test bgCancelshred">Supprimer la musique</button>
</form>
